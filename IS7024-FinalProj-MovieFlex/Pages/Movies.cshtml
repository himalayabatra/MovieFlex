@page
@model IS7024_FinalProj_MovieFlex.Pages.MovieDemoModel
@using MovieJoin
@{
    ViewData["Title"] = "MovieFlex Moviedemo";
    MovieDemo moviedemos = (MovieDemo)ViewData["moviedemos"];
}

<div class="text-center">
    <h1 class="display-4">MoveiFlex Top 250 Shows</h1>
    <br />
    <table class="table table-dark table-striped">
        <tr>
            <th>DisplatTitle</th>
            <th>CriticsPick</th>

        </tr>
        @{
            foreach (Result status in moviedemos.Results)
            {
                <tr>

                    <td>@status.DisplayTitle</td>
                    <td>@status.CriticsPick</td>

                </tr>
            }
        }
    </table>
</div>